{
  "buildCommand": "npx vite build",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps",
  "env": {
    "NODE_VERSION": "20.x"
  },
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}